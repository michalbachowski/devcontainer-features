{
    "from_opt": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "custom-ca-cert": {
                "cert_path": "/optcert.pem"
            }
        }
    },
    "from_env": {
        "image": "michalbachowski/devcontainer-test-ubuntu:v1",
        "features": {
            "custom-ca-cert": {}
        }
    },
    "from_both": {
        "image": "michalbachowski/devcontainer-test-ubuntu:v1",
        "features": {
            "custom-ca-cert": {
                "cert_path": "/optcert.pem"
            }
        }
    }
}
