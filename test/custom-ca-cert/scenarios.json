{
    "custom_alias": {
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3",
        "features": {
            "custom-ca-cert": {
                "keytool-cert-alias": "My Alias"
            }
        }
    },
    "update_onCreateCommand": {
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3",
        "features": {
            "custom-ca-cert": {
                "update_cert_stores_during_build": true
            }
        }
    },
    "custom_alias_and_onCreateCommand": {
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3",
        "features": {
            "custom-ca-cert": {
                "keytool-cert-alias": "My Alias",
                "update_cert_stores_during_build": true
            }
        }
    }
}
