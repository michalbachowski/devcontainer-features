{
    "non-root-user": {
        "features": {
            "git-commit-signing": {
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3",
        "remoteUser": "testuser"
    },
    "format-x509": {
        "features": {
            "git-commit-signing": {
                "key_format": "x509"
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"
    },
    "format-gpg": {
        "features": {
            "git-commit-signing": {
                "key_format": "gpg"
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"
    },
    "custom-allowed-signers": {
        "features": {
            "git-commit-signing": {
                "allowed_signers_file": "/bar"
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"
    },
    "allowed-signers-and-force-verify-on-merge": {
        "features": {
            "git-commit-signing": {
                "allowed_signers_file": "/foo",
                "force_verification_on_merge": true
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"

    },
    "allowed-signers-and-no-force-verify-on-merge": {
        "features": {
            "git-commit-signing": {
                "allowed_signers_file": "/foo",
                "force_verification_on_merge": false
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"

    },
    "no-allowed-signers-and-no-force-verify-on-merge": {
        "features": {
            "git-commit-signing": {
                "force_verification_on_merge": false
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"
    },
    "no-allowed-signers-and-force-verify-on-merge": {
        "features": {
            "git-commit-signing": {
                "force_verification_on_merge": true
            }
        },
        "image": "bachowskimichal/devcontainer-test-ubuntu:v3"
    }
}
