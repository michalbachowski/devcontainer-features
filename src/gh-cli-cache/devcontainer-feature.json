{
    "name": "GitHub CLI (gh) cache",
    "id": "gh-cli-cache",
    "version": "0.0.1",
    "description": "Prepares a volume to cache [gh] cli credentials.",
    "installsAfter": [
    ],
    "containerEnv": {
		"GH_CONFIG_DIR": "/devcontainer-feature/michalbachowski/gh-cli-cache"
	},
    "mounts": [
        {
            "source": "sbt-cache",
            "target": "/devcontainer-feature/michalbachowski/gh-cli-cache",
            "type": "volume"
        }
    ]
}
