{
    "name": "sbt cache",
    "id": "sbt-cache",
    "version": "0.0.2",
    "description": "Prepares a volume to cache sbt (coursier) packages.",
    "installsAfter": [
    ],
    "containerEnv": {
		"COURSIER_CACHE": "/devcontainer-feature/michalbachowski/sbt-cache/coursier"
	},
    "mounts": [
        {
            "source": "sbt-cache",
            "target": "/devcontainer-feature/michalbachowski/sbt-cache/coursier",
            "type": "volume"
        }
    ]
}
